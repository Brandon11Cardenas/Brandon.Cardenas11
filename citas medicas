CREATE TABLE citas (
  numero int NOT NULL,
  fecha DATE NOT NULL,
  hora int NOT NULL,
  paciente int NOT NULL,
  medico varchar(50) NOT NULL,
  consultorio int NOT NULL,
  estado varchar(50),
  observaciones varchar(1000),
  PRIMARY KEY (numero)
);

