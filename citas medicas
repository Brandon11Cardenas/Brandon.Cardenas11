CREATE TABLE pacientes (
  identificacion int NOT NULL,
  nombres varchar(50) NOT NULL,
  apellidos varchar(50) NOT NULL,
  fechaNacimiento date NOT NULL,
  sexo varchar(15) NOT NULL,
  PRIMARY KEY (identificacion)
);

