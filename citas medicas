CREATE TABLE tratamientos (
  numero int NOT NULL,
  fechaAsignado DATE NOT NULL,
  descripcion varchar(1000),
  fechaInicio DATE NOT NULL,
  fechaFin DATE NOT NULL,
  observaciones varchar(1000),
  paciente int NOT NULL,
  PRIMARY KEY (numero)
);  

